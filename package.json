    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-plugin-webpack-aliases": "^1.1.3",
